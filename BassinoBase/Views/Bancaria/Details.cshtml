@using BassinoLibrary.Helpers
@using BassinoLibrary.Resource
@model BassinoLibrary.ViewModels.BancariaViewModel

@{
    Layout = "~/Views/Shared/_ModalLayout.cshtml";
}

@section Header
{
    @Helpers.ModalHeaderDetails(this, "Bancaria", Model.Id, string.Format("{0} {1}", Resources.Details, "Bancaria"))
}


<fieldset>
    @Html.HiddenFor(model => model.Id)

    @Html.PersonalDisplayFor(model => model.Entidad)
    @Html.PersonalDisplayFor(model => model.NumeroCuenta)
    @Html.PersonalDisplayFor(model => model.Saldo)

</fieldset>


