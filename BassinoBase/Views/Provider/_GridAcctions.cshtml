@using BassinoLibrary.Resource
@if (ViewBag.editar)
{
    <a href="/Provider/Edit/#= Id #" data-target="\\#modal" data-toggle="modal" data-placement="top" title="Responder"><i class="fa fa-pencil fa-2x"></i></a>
}
@if (ViewBag.borrar)
{
    <a href="/Provider/Delete/#= Id #" data-target="\\#modal" data-toggle="modal" data-placement="top" title="Leer"><i class="fa fa-trash fa-2x"></i></a>
}
@if (ViewBag.imprimir)
{
    <a href="/Report/PrintProviderById/#= Id #" target="_blank" data-placement="top" title="Imprimir"><i class="fa fa-print fa-2x"></i></a>
}





