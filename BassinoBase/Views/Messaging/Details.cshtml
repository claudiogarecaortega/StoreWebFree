@using BassinoLibrary.Helpers
@using BassinoLibrary.Resource
@model BassinoLibrary.ViewModels.MessagingViewModel

@{
    Layout = "~/Views/Shared/_ModalLayout.cshtml";
}

@section Header
{
    @Helpers.ModalHeaderDetails(this, "MessagingViewModel", Model.Id, string.Format("{0} {1}", Resources.Details, Resources.Messaging))
}


<fieldset>

	@Html.PersonalDisplayFor(model => model.UserSend)
	@Html.PersonalDisplayFor(model => model.Subject)
	@Html.PersonalDisplayFor(model => model.Message)

</fieldset>


