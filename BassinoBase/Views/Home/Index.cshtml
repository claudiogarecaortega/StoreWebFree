@using BassinoBase.Controllers
@using BassinoBase.Models
@using BassinoLibrary.Helpers
@using BassinoLibrary.Resource

<script>
  
    $(function () {
        // Reference the auto-generated proxy for the hub.
        var chat = $.connection.noficationsHub;
        chat.client.nofifyClientAction = function (id, ids) {

            var iuse = $("#idUser").val();
            if (id > 0 && iuse != ids) {
                $.ajax({
                    type: "POST",
                    url: "/Notification/GetNotification",
                    datatype: "JSON",
                    data: { text: id },
                    success: function (data) {
                        var numeroNoti = Number($("#notificationNumber").text());
                        var total = numeroNoti + 1;
                        $("#notificationNumber").text(total);
                        $("#notificationLabel").text("Ud tiene "+total+" notificaciones");
                        if(data.Importance==3){
                            var mlin = "<li id='notili_" + data.Id + "'><a href=" + data.Url + "  onclick='notificationReadToolbar(" + data.Id + ")'  data-target='#modal' data-toggle='modal' data-placement='top'><i class='fa fa-warning text-yellow'></i>  " + data.Message + "</a></li>";
                            $("#notificationList").append(mlin);
                            alertUrgentMessage("Ud tiene una Notificacion urgente en su bandea de entrada");
                        }
                        if (data.Importance == 4) {
                            var mlin = "<li id='notili_" + data.Id + "'><a href=" + data.Url + " onclick='notificationReadToolbar(" + data.Id + ")'  data-target='#modal' data-toggle='modal' data-placement='top'><i class='fa fa-ban text-red'></i>  " + data.Message + "</a></li>";
                            $("#notificationList").append(mlin);
                            alertUrgentMessage("Ud tiene una Notificacion urgente en su bandea de entrada");
                        }

                        if (data.Importance == 2) {
                            var mlin = "<li id='notili_" + data.Id + "'><a href=" + data.Url + " onclick='notificationReadToolbar(" + data.Id + ")'  data-target='#modal' data-toggle='modal' data-placement='top'><i class='fa fa-info text-blue'></i>  " + data.Message + "</a></li>";
                            $("#notificationList").append(mlin);
                            alertUrgentMessage("Ud tiene una Notificacion urgente en su bandea de entrada");
                        }

                        if (data.Importance == 1) {
                            var mlin = "<li id='notili_" + data.Id + "'><a href=" + data.Url + " onclick='notificationReadToolbar(" + data.Id + ")'  data-target='#modal' data-toggle='modal' data-placement='top'><i class='fa fa-check text-yellow'></i>  " + data.Message + "</a></li>";
                            $("#notificationList").append(mlin);
                            alertUrgentMessage("Ud tiene una Notificacion urgente en su bandea de entrada");
                        }

                    }
                });
            }
        };
        // Create a function that the hub can call back to display messages.
        chat.client.addNewMessageToPage = function (id) {

            var idusuario = $("#idUser").val();
            var result = jQuery.inArray(idusuario, id);
            if ( result==0) {
                var valueLi = "";
                var numero = Number($("#valueNotification").text());
                numero += 1;
                $("#valueNotification").text(numero);
                $("#liheader").text("Ud tiene : "+numero+" mensajes");
                $.ajax({
                    type: "POST",
                    url: "/Messaging/GetNewMessages",
                    datatype: "JSON",
                    success: function (data) {
                        $("#messageList").empty();
                        $.each(data, function (i, item) {

                            if(item.IsUrgent) {

                                valueLi ="<li><a href='/Messaging/Details/"+item.id+"'  data-target='#modal' data-toggle='modal' data-placement='top'><div class='pull-left'><img src='dist/img/user2-160x160.jpg' class='img-circle' alt='User Image'></div><h4>" + item.from + "<small><i class='fa fa-clock-o'></i>" + item.time + "</small></h4><p>" + item.subject + "</p></a></li>";

                                $('#messageList').append(valueLi);
                                alertUrgentMessage("Ud tiene un mensaje urgente en su bandea de entrada");
                            }
                            else{
                                valueLi ="<li style='background-color:red'><a href='/Messaging/Details/"+item.id+"'  data-target='#modal' data-toggle='modal' data-placement='top'><div class='pull-left'><img src='dist/img/user2-160x160.jpg' class='img-circle' alt='User Image'></div><h4>" + item.from + "<small><i class='fa fa-clock-o'></i>" + item.time + "</small></h4><p>" + item.subject + "</p></a></li>";
                                $('#messageList').append(valueLi);
                            }
                        });

                    }
                });

            }
        };
        $.connection.hub.start();

    });


</script>

