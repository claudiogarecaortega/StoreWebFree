@using BassinoLibrary.Resource
@if (ViewBag.editar)
{
    <a href="/Client/Edit/#= Id #" data-target="\\#modal" data-toggle="modal" data-backdrop='static' data-keyboard='false' data-placement="top" title="Responder"><i class="fa fa-pencil fa-2x"></i></a>
}
@if (ViewBag.borrar)
{
    <a href="/Client/Delete/#= Id #" data-target="\\#modal" data-toggle="modal" data-backdrop='static' data-keyboard='false' data-placement="top" title="Leer"><i class="fa fa-trash fa-2x"></i></a>
}
@if (ViewBag.imprimir)
{
    <a href="/Report/PrintClientById/#= Id #" target="_blank" data-placement="top" data-backdrop='static' data-keyboard='false' title="Imprimir"><i class="fa fa-print fa-2x"></i></a>
}





