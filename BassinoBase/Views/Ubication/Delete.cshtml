@using BassinoLibrary.Helpers
@using BassinoLibrary.Resource
@using BassinoLibrary.ViewModels

@model BassinoLibrary.ViewModels.UbicationViewModel

@{
    Layout = "~/Views/Shared/_ModalLayout.cshtml";
}

@section Header
{
    @Helpers.ModalHeader(string.Format("{0} {1}", Resources.Delete, Resources.Ubication))
}

<fieldset>

	@Html.HiddenFor(model => model.Id)
				
	@Html.PersonalDisplayFor(model => model.Description)
	@Html.PersonalDisplayFor(model => model.UbicacionPadre)
   
				
</fieldset>

@section Footer
{
    @Helpers.BotonesBorrarCancelar()
}
